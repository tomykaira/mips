.PHONY: all clean check
OCAMLINCLUDES += ../src

FILES[] =
	test_alpha
	test_reg
	test_registerAllocation
	test_schedule

OCAMLPACKS += oUnit

PROGRAM = test
OCAML_LIBS += ../src/ccc

clean:
	rm -f *~ *.opt *.cmi *.cmx *.o *.omc *.cma *.cmxa

.DEFAULT: all

all : $(OCamlProgram $(PROGRAM), $(FILES))

check : all
	./$(PROGRAM)
